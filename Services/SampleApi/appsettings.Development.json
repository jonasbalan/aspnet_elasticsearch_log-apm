{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },

  "ElasticConfiguration": {
    "Uri": "http://elasticsearch:9200",
    "ElasticApm": {
      "SecretToken": "",
      "ServerUrls": "http://elastic-apm:8200", //Set custom APM Server URL (default: http://localhost:8200)
      "ServiceName": "SampleApi-JFB"
    }
  },

  "management": {
    "tracing": {
      "exporter": {
        "zipkin": {
          "endpoint": "http://localhost:9411/api/v2/spans",
          "validateCertificates": false,
          "timeoutSeconds": 3,
          "serviceName": "api1",
          "useShortTraceIds": true
        }
      }
    }
  }


}
